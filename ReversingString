public class ReversingString {
    public static void main(String args[]) {
     String s = "Sherlin";
     String rev = "";
     char[] chArr = s.toCharArray();
     System.out.println("The String is: "+s);
     for(int i=chArr.length-1; i>=0; i--)
     {
        rev = rev + chArr[i];
     }
     System.out.println("Reverse of the sting is: " +rev);
    }
}
